        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.27.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "jsdoc": "^3.6.7",
        "json-server": "^0.17.0",
        "prettier": "^2.4.1",
        "webpack": "^4.46.0",
        "webpack-dev-server": "^4.5.0"
